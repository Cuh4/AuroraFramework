{
    "Lua.diagnostics.disable": [
        "lowercase-global",
        "undefined-doc-name"
    ],
    "Lua.runtime.version": "Lua 5.3",
    "Lua.runtime.builtin": {
        "coroutine": "enable",
        "bit32": "enable",
        "bit": "enable",
        "builtin": "enable",
        "utf8": "enable",
        "package": "enable",
        "os": "enable",
        "jit": "enable",
        "io": "enable",
        "ffi": "enable",
        "debug": "enable",
        "basic": "enable"
    },
    "Lua.workspace.ignoreDir": [
        ".vscode",
    ],
    "Lua.runtime.path": [
        "?.lua",
        "?/init.lua"
    ],
    "Lua.runtime.pathStrict": true,
    "discord.enabled": true,
    "editor.inlayHints.fontSize": 11
}